export BINDIR=$HOME/bin
export NIOS2NOMMUBINDIR=/opt/nios2/bin
export NIOS2MMUBINDIR=$HOME/src/nios2-linux/toolchain-mmu/x86-linux2/bin
export ALTERADIR=/opt/altera9.1

